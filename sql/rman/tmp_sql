TO_CHAR(END_TIME,'yyyy-mm-dd hh24:mi:ss') END_TIME,
INPUT_BYTES,
OUTPUT_BYTES,
STATUS_WEIGHT,
OPTIMIZED_WEIGHT,
INPUT_TYPE_WEIGHT,
OUTPUT_DEVICE_TYPE,
AUTOBACKUP_COUNT,
BACKED_BY_OSB,
AUTOBACKUP_DONE,
STATUS,
OPTIMIZED,
ELAPSED_SECONDS,
COMPRESSION_RATIO,
INPUT_BYTES_PER_SEC,
OUTPUT_BYTES_PER_SEC,
INPUT_BYTES_DISPLAY,
OUTPUT_BYTES_DISPLAY,
INPUT_BYTES_PER_SEC_DISPLAY,
OUTPUT_BYTES_PER_SEC_DISPLAY
FROM RC_RMAN_BACKUP_JOB_DETAILS;





